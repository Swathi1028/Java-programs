import java.io.*;
import java.lang.*;

class GfG {
    public static void main(String[] args) {
        // starting number
        int a = 2;

        // Common difference
        int d = 1;

        // N th term to be find
        int n = 5;

        int term = a + (n - 1) * d;
        // Display the output
        System.out.print(n + "th term of the series is : "
                         + term);
    }
}
