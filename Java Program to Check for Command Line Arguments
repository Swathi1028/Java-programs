class GfG {

    // Main driver method
    public static void main(String[] args) {

        // Checking if length of args array is
        // greater than 0
        if (args.length > 0) {
            System.out.println("Arguments are");
          
            // Iterating the args array
            // using for each loop
            for (String x : args)
                // Printing command line arguments
                System.out.print(x + " ");
        }
        else {
            System.out.println("No Arguments");
        }
    }
}
