public class GfG {
    public static void main(String[] args) {
        int x = 10, y = 20;

        // Addition
        System.out.println("Addition: " + (x + y)); 

        // Subtraction
        System.out.println("Subtraction: " + (x - y)); 

        // Multiplication
        System.out.println("Multiplication: " + (x * y));

        // Division
        System.out.println("Division: " + (x / y));

        // Modulo
        System.out.println("Modulo: " + (x % y));
    }
}
