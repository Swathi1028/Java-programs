class Solution {
    public List<String> readBinaryWatch(int turnedOn) {
        List<String> ans = new ArrayList<String>();
        for(int i=0;i<12;++i){
            for(int m = 0;m<60;++m){
                if(Integer.bitCount(i) + Integer.bitCount(m) == turnedOn){
                    ans.add(i + ":" + (m < 10 ? "0" : "") + m);
                }
            }
        }
        return ans;
    }
}
